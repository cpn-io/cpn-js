{
  "name": "cpn-ide",
  "version": "1.0.1",
  "author": "INDEV",
  "main": "electron/dist/main.js",
  "scripts": {
    "ng": "ng",
    "start": "node ./version.build.js & ng serve --proxy-config proxy.conf.json",
    "updateBuild": "node ./replace.build.js",
    "build": "node ./version.build.js & ng build",
    "buildProd": "node ./version.build.js & ng build --prod --build-optimizer=false --base-href ./",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "docs": "typedoc --options typedoc.json ./src/app/ ",
    "compodoc": "compodoc -p tsconfig.json",
    "buildBackendLinux": "./../backend/gradlew clean -p ./../backend & ./../backend/gradlew build -p ./../backend",
    "buildBackendWin": ".\\..\\backend\\gradlew clean -p .\\..\\backend & .\\..\\backend\\gradlew build -p .\\..\\backend",
    "electronBuild": "tsc --p electron",
    "electronCleanLinux": "rm -rf ./electron/dist & rm -rf ./dist",
    "electronCleanWin": "rmdir /S /Q \".\\electron\\dist\" & rmdir /S /Q \".\\dist\"",
    "electronBuildAllLinux": "npm run -s electronCleanLinux & npm run buildBackendLinux & npm run buildProd & npm run electronBuild",
    "electronBuildAllWin": "npm run -s electronCleanWin & npm run buildBackendWin & npm run buildProd & npm run electronBuild",
    "electronRun": "electron . > electron-app.log",
    "electronCopyWinUnpacked": "xcopy \".\\dist\\win-unpacked\\*\" \".\\electron\\installer\\win-unpacked\\*\" /s /e /y",
    "electronMakeSetupLinux": "electron-builder",
    "electronMakeSetupWin": "electron-builder & npm run electronCopyWinUnpacked",
    "electronBuildAndMakeSetupWin": "npm run electronBuildAllWin & npm run electronMakeSetupWin"
  },
  "build": {
    "appId": "cpn-ide",
    "extends": null,
    "win": {
      "sign": "./customSign.js",
      "target": "msi"
    },
    "files": [
      "./dist/**/*",
      "./electron/dist/main.js",
      "./electron/splash.html",
      "./electron/splash.png"
    ],
    "extraResources": [
      {
        "from": "./electron/backend",
        "to": "backend",
        "filter": [
          "**/*"
        ]
      }
    ]
  },
  "private": true,
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.23",
    "@angular/cli": "^8.3.23",
    "@angular/compiler-cli": "~8.2.5",
    "@angular/language-service": "~8.2.5",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "^2.0.8",
    "@types/jquery": "^3.3.31",
    "@types/node": "^10.17.13",
    "advanced-json-path": "^1.0.8",
    "bootstrap": "^4.4.1",
    "codelyzer": "~4.5.0",
    "electron": "^3.1.13",
    "electron-builder": "^20.44.4",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.6",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.4.2",
    "replace-in-file": "^3.4.4",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typedoc": "^0.15.8",
    "typescript": "^3.5.1"
  },
  "dependencies": {
    "@angular/animations": "~8.2.5",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "~8.2.5",
    "@angular/compiler": "~8.2.5",
    "@angular/core": "~8.2.5",
    "@angular/forms": "~8.2.5",
    "@angular/http": "~7.0.0",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.5",
    "@angular/router": "~8.2.5",
    "@fortawesome/angular-fontawesome": "^0.3.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@ng-bootstrap/ng-bootstrap": "^4.2.2",
    "@progress/kendo-angular-l10n": "^1.4.1",
    "@progress/kendo-angular-scrollview": "^2.2.2",
    "@types/file-saver": "^1.3.0",
    "angular-fontawesome": "^1.0.0",
    "angular-split": "^2.0.1",
    "angular-tree-component": "^8.5.2",
    "circular-json": "^0.5.9",
    "core-js": "^2.6.11",
    "dateformat": "^3.0.3",
    "deep-diff": "^1.0.2",
    "deep-equal": "^1.1.1",
    "diagram-js": "^3.3.1",
    "electron-is-dev": "^1.1.0",
    "electron-log": "^3.0.9",
    "electron-splashscreen": "^1.0.0",
    "file-saver": "^1.3.8",
    "find-process": "^1.4.3",
    "flex-layout": "^1.0.1",
    "golden-layout": "^1.5.9",
    "ids": "^1.0.0",
    "jquery": "^3.4.1",
    "mat-icon-import": "^0.1.1",
    "moddle": "^4.1.0",
    "moddle-xml": "^7.5.0",
    "ng-bootstrap": "^1.6.3",
    "ng2-json-editor": "^0.25.13",
    "ng2-json-view": "^4.0.2",
    "ngx-electron": "^1.0.4",
    "ngx-json-viewer-scrolling": "^2.3.1",
    "ngx-xml2json": "^1.0.2",
    "popper.js": "^1.16.1",
    "ps-node": "^0.1.6",
    "rxjs": "^6.5.4",
    "rxjs-compat": "^6.5.4",
    "rxjs-observe": "^2.1.5",
    "ts-helpers": "^1.1.2",
    "typeface-droid-sans-mono": "0.0.44",
    "xml-js": "^1.6.11",
    "xmldom": "^0.1.31",
    "zone.js": "^0.8.29"
  }
}
