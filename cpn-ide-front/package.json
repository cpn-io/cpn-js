{
  "name": "cpn-ide",
  "version": "1.22.1117",
  "author": "TU/e",
  "main": "electron/dist/main.js",
  "scripts": {
    "ng": "ng",
    "pretty": "prettier --write \"./**/*.ts\"",
    "start": "node ./version.build.js & ng serve --proxy-config proxy.conf.json",
    "updateBuild": "node ./replace.build.js",
    "build": "node ./version.build.js & ng build",
    "buildProd": "node ./version.build.js & ng build --prod --build-optimizer=false --base-href ./",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "docs": "typedoc --options typedoc.json ./src/app/ ",
    "buildBackendLinux": "./../backend/gradlew clean -p ./../backend & ./../backend/gradlew build -p ./../backend",
    "buildBackendWin": ".\\..\\backend\\gradlew clean -p .\\..\\backend & .\\..\\backend\\gradlew build -p .\\..\\backend",
    "electronBuild": "tsc --p electron",
    "electronCleanLinux": "rm -rf ./electron/dist & rm -rf ./dist",
    "electronCleanWin": "rmdir /S /Q \".\\electron\\dist\" & rmdir /S /Q \".\\dist\"",
    "electronBuildAllLinux": "npm run -s electronCleanLinux & npm run buildBackendLinux & npm run buildProd & npm run electronBuild",
    "electronBuildAllWin": "npm run -s electronCleanWin & npm run buildBackendWin & npm run buildProd & npm run electronBuild",
    "electronRun": "electron . > electron-app.log",
    "electronRenameWinUnpacked": "rename \".\\dist\\win-ia32-unpacked\" \"win-unpacked\"",
    "electronCopyWinUnpacked": "npm run electronRenameWinUnpacked & xcopy \".\\dist\\win-unpacked\\*\" \".\\electron\\installer\\win-unpacked\\*\" /s /e /y",
    "electronMakeSetupLinux": "electron-builder",
    "electronMakeSetupWin": "electron-builder & npm run electronCopyWinUnpacked",
    "electronBuildAndMakeSetupWin": "npm run electronBuildAllWin & npm run electronMakeSetupWin"
  },
  "build": {
    "appId": "cpn-ide",
    "extends": null,
    "win": {
      "sign": "./customSign.js",
      "target": "msi"
    },
    "files": [
      "./dist/**/*",
      "./electron/dist/main.js",
      "./electron/splash.html",
      "./electron/splash.png"
    ],
    "extraResources": [
      {
        "from": "./electron/backend",
        "to": "backend",
        "filter": [
          "**/*"
        ]
      }
    ]
  },
  "private": true,
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.9",
    "@angular/cli": "^9.1.9",
    "@angular/compiler-cli": "~9.1.11",
    "@angular/language-service": "~9.1.11",
    "@compodoc/compodoc": "^1.1.18",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "^2.0.8",
    "@types/jquery": "^3.5.0",
    "@types/node": "^12.11.1",
    "advanced-json-path": "^1.0.8",
    "bootstrap": "^4.5.0",
    "codelyzer": "^5.1.2",
    "electron": "^3.1.13",
    "electron-builder": "^20.44.4",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^5.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.6",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^7.0.0",
    "replace-in-file": "^3.4.4",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typedoc": "^0.22.11"
  },
  "dependencies": {
    "@angular/animations": "^9.1.11",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "~9.1.11",
    "@angular/compiler": "~9.1.11",
    "@angular/core": "~9.1.11",
    "@angular/forms": "~9.1.11",
    "@angular/http": "~7.0.0",
    "@angular/localize": "^9.1.11",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "^9.1.11",
    "@angular/platform-browser-dynamic": "~9.1.11",
    "@angular/router": "~9.1.11",
    "@fortawesome/angular-fontawesome": "^0.3.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.29",
    "@fortawesome/free-solid-svg-icons": "^5.13.1",
    "@ng-bootstrap/ng-bootstrap": "^4.2.2",
    "@types/file-saver": "^1.3.0",
    "angular-fontawesome": "^1.0.0",
    "angular-split": "^3.0.3",
    "angular-tree-component": "^8.5.6",
    "circular-json": "^0.5.9",
    "core-js": "^2.6.11",
    "dateformat": "^3.0.3",
    "deep-diff": "^1.0.2",
    "deep-equal": "^1.1.1",
    "diagram-js": "^3.3.1",
    "electron-is-dev": "^1.2.0",
    "electron-log": "^3.0.9",
    "electron-splashscreen": "^1.0.0",
    "file-dialog": "0.0.8",
    "file-saver": "^1.3.8",
    "find-process": "^1.4.3",
    "flex-layout": "^1.0.1",
    "golden-layout": "^1.5.9",
    "ids": "^1.0.0",
    "jquery": "^3.5.1",
    "mat-icon-import": "^0.1.1",
    "moddle": "^4.1.0",
    "moddle-xml": "^7.5.0",
    "ngx-electron": "^1.0.4",
    "ngx-xml2json": "^1.0.2",
    "popper.js": "^1.16.1",
    "prettier": "^2.2.1",
    "ps-node": "^0.1.6",
    "rxjs": "^6.5.5",
    "rxjs-compat": "^6.5.5",
    "rxjs-observe": "^2.1.5",
    "safe-pipe": "^1.0.3",
    "ts-helpers": "^1.1.2",
    "tslib": "^1.10.0",
    "typeface-droid-sans-mono": "0.0.44",
    "typescript": "^3.8.3",
    "xml-js": "^1.6.11",
    "xmldom": "^0.1.31",
    "zone.js": "~0.10.2"
  }
}
